							"component": "textfield",
							"value": "Application Name",
							"mandatory": 1,
							"target": "manifest",
							"type": "string",
							"params": "{ \"width\": 620,\"height\": 28 }"
						},
						{
							"id": "APP_VERSION",
							"name": "Version",
							"component": "textfield",
							"value": "1.0.0",
