						{
							"id": "APP_COPYRIGHT",
							"name": "Copyright",
							"component": "textfield",
							"value": "(c)2020 My Company.",
							"mandatory": 1,
							"target": "manifest",
							"type": "string",
							"params": "{ \"width\": 620,\"height\": 28}"
						},
						{
							"id": "APP_AUTHOR",
							"name": "Author",
							"component": "textfield",
							"value": "Your name",
							"mandatory": 1,
							"target": "manifest",
							"type": "string",
							"params": "{ \"width\": 620,\"height\": 28}"
						},
						{
							"component": "separator"
