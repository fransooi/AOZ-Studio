							"target": "icon",
							"type": "string",
							"params": "{ \"width\": 472,\"height\": 28 }"
						}
					]
				},
				{
					"title": "Display parameters",
					"items": [
						{
							"id": "APP_WIDTH",
							"name": "Width",
							"component": "textfield",
							"value": "1920",
							"mandatory": 1,
							"target": "manifest",
							"type": "int",
							"params": "{ \"x\": 200, \"width\": 200,\"height\": 28 }"
						},
						{
							"id": "APP_HEIGHT",
							"name": "Height",
							"component": "textfield",
							"value": "1080",
							"mandatory": 1,
							"target": "manifest",
							"type": "int",
							"params": "{ \"x\": 200, \"width\": 200,\"height\": 28 }"
						},
						{
							"id": "APP_KEEP_PROPORTIONS",
							"name": "Keep Proportions",
							"component": "checkbox",
							"value": "true",
							"target": "manifest",
							"type": "bool",
							"params": "{ \"width\": 400 }"
						},
						{
							"id": "SMOOTH_ON",
							"name": "Smoothing Image",
							"component": "checkbox",
							"value": "true",
							"target": "manifest",
							"type": "bool",
							"params": "{ \"width\": 400 }"
						},
						{
							"component": "separator"
						},
						{
							"id": "APP_SCREEN_PARAMS",
							"name": "Screen Parameters",
							"component": "label",
							"params": "{ \"width\": 620}"
						},
						{
							"id": "APP_FPS",
							"name": "Show the FPS",
							"component": "checkbox",
							"value": "false",
							"target": "manifest",
							"type": "bool",
							"params": "{ \"width\": 400 }"
