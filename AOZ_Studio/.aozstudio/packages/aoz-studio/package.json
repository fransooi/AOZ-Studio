{
  "name": "aoz-studio",
  "main": "./lib/aoz-studio",
  "version": "0.0.0",
  "description": "Ths AOZ Studio Atom integration package",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "aoz-studio:toggle",
      "aoz-studio:new-file",
      "aoz-studio:load-file",
      "aoz-studio:save-file",
      "aoz-studio:import-amos",
      "aoz-studio:run-atom",
      "aoz-studio:run-browser",
      "aoz-studio:run-tv",
      "aoz-studio:stop-tv",
      "aoz-studio:test",
      "aoz-studio:previous-error",
      "aoz-studio:next-error",
      "aoz-studio:help-instruction",
      "aoz-studio:publish",
      "aoz-studio:aoz-designer",
      "aoz-studio:set-developer-mode-on",
      "aoz-studio:set-developer-mode-off"
    ]
  },
  "repository": "https://github.com/atom/aoz-studio",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "getProvider"
      }
    }
  },
  "configSchema": {
    "enableAutoComplete": {
      "title": "Enable Auto Complete",
      "description": "Display possible completions in the editor while typing.",
      "type": "boolean",
      "default": true,
      "order": 1
    },
    "minimumWordLength": {
      "description": "Only show suggestions when you've typed at least this many characters.",
      "type": "integer",
      "default": 3,
      "order": 2
    },
    "includeAdditionalSuggestions": {
      "description": "Include additional suggestions such as variables, labels etc",
      "type": "boolean",
      "default": true,
      "order": 3
    }
  },
  "dependencies": {
    "btoa": "^1.2.1",
    "child_process": "^1.0.2",
    "child-process": "^1.0.2",
    "date-and-time": "^0.14.2",
    "express": "^4.17.1",
    "font-list": "^1.2.11",
    "fs-plus": "^3.1.1",
    "ftp-deploy": "^2.3.7",
    "get-user-locale": "^1.3.0",
    "hjson": "^3.2.1",
    "jszip": "^3.2.2",
    "live-server": "^1.2.1",
    "lodash": "^4.17.15",
    "mixto": "^1.0.0",
    "mkdirp": "^1.0.3",
    "node-process-windows": "0.0.2",
    "obfuscator": "^0.0.2",
    "object.fromentries": "^2.0.2",
    "path": "^0.12.7",
    "pngjs": "^5.0.0",
    "ps-node": "^0.1.6",
    "read-ini-file": "^3.0.0",
    "request": "^2.88.2",
    "systeminformation": "^4.23.8",
    "theorist": "^1.0.2",
    "tmp": "^0.1.0",
    "ws": "^7.3.0"
  }
}
